	"bugs": {
		"url": "https://github.com/cyrale/block-manager-for-block-editor/issue"
	},
	"dependencies": {},
	"devDependencies": {
		"@wordpress/api-fetch": "3.17.0",
		"@wordpress/base-styles": "1.10.0",
		"@wordpress/blocks": "6.18.0",
		"@wordpress/components": "9.8.0",
		"@wordpress/compose": "3.17.0",
		"@wordpress/data": "4.20.0",
		"@wordpress/dom-ready": "2.9.0",
		"@wordpress/element": "2.14.0",
		"@wordpress/env": "1.5.0",
		"@wordpress/eslint-plugin": "7.0.0",
		"@wordpress/hooks": "2.8.0",
		"@wordpress/i18n": "3.13.0",
		"@wordpress/prettier-config": "0.2.0",
		"@wordpress/primitives": "1.5.0",
		"@wordpress/scripts": "11.0.0",
		"@wordpress/url": "2.16.0",
		"classnames": "2.2.6",
		"concurrently": "5.2.0",
		"eslint": "6.8.0",
		"husky": "3.0.5",
		"lint-staged": "10.2.11",
		"lodash": "4.17.15",
		"prettier": "npm:wp-prettier@2.0.5",
		"puppeteer": "2.1.1",
		"react": "16.11.0",
		"react-accessible-accordion": "3.0.1",
		"react-dom": "16.11.0",
		"react-native": "0.62.2",
		"react-tabs": "3.1.1",
		"react-test-renderer": "16.9.0",
		"rimraf": "3.0.2",
		"stylelint": "13.6.1",
		"stylelint-config-rational-order": "0.1.2",
		"stylelint-config-wordpress": "17.0.0",
		"stylelint-order": "4.1.0",
		"typescript": "3.9.5"
	},
	"scripts": {
		"build": "wp-scripts build ./src/*.js",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "concurrently \"wp-scripts check-licenses --prod --gpl2\" \"wp-scripts check-licenses --dev\"",
		"clean": "rimraf ./build",
		"format:css": "npm run lint:css -- --fix",
		"format:js": "wp-scripts format-js",
		"format:php": "composer run-script format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"lint:md:docs": "wp-scripts lint-md-docs",
		"lint:md:js": "wp-scripts lint-md-js",
		"lint:php": "composer run-script lint",
		"lint:pkg-json": "wp-scripts lint-pkg-json",
		"packages-update": "wp-scripts packages-update",
		"start": "wp-scripts start ./src/*.js",
		"wp-env": "wp-env"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"src/**/*.js": [
			"npm run format:js",
			"npm run lint:js"
		],
		"src/**/*.scss": [
			"npm run format:css",
			"npm run lint:css"
		],
		"*.php": [
			"composer run-script format",
			"composer run-script lint"
		]
	}
}
